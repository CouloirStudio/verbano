{
  "name": "verbano-app",
  "version": "1.0.0",
  "description": "Audio transcription and report generation application",
  "private": true,
  "scripts": {
    "typedoc": "typedoc --options typedoc.json --skipErrorChecking",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'server/**/*' -e ts,tsx --exec 'ts-node' --project server/tsconfig.server.json server/server.ts",
    "build": "next build && yarn build:server",
    "build:server": "tsc --project ./server/tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node server/build/server/server.js",
    "test:frontend": "jest --config jest.frontend.config.js --runInBand  --detectOpenHandles",
    "test:backend": "jest --config jest.backend.config.cjs --runInBand  --detectOpenHandles",
    "test": "concurrently \"yarn dev\" \"yarn test:frontend && yarn test:backend && cypress open\"",
    "cypress:open": "cypress open",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/CouloirStudio/verbano.git"
